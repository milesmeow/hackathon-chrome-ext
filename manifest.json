{
    "manifest_version":2,
    "name": "Fun Pac",
    "version": "1.0",
    "description": "Search utility and Pac Man activity",
    "content_scripts":[
        {
            "matches" : ["*://*/*"],
            "js":["jquery.min.js","./main.js"],
            "css" : ["style.css"]
        }
    ],
    "web_accessible_resources": [
        "audio/*.wav",
        "images/*.png",
        "images/*.gif"
    ]
}